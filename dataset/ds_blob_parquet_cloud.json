{
	"name": "ds_blob_parquet_cloud",
	"properties": {
		"description": "Dataset used to save parquet files to the internal data factory storage account",
		"linkedServiceName": {
			"referenceName": "ls_blob_cloud",
			"type": "LinkedServiceReference",
			"parameters": {
				"storage_account_uri": {
					"value": "@dataset().storage_account_uri",
					"type": "Expression"
				}
			}
		},
		"parameters": {
			"storage_account_uri": {
				"type": "string"
			},
			"container_name": {
				"type": "string"
			},
			"datastore_name": {
				"type": "string"
			},
			"dataset_name": {
				"type": "string"
			},
			"file_name": {
				"type": "string"
			}
		},
		"folder": {
			"name": "azure_shir"
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().file_name",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('datastore_', dataset().datastore_name, '/dataset_', dataset().dataset_name)",
					"type": "Expression"
				},
				"container": {
					"value": "@dataset().container_name",
					"type": "Expression"
				}
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}
